/* Actors */
clock : misc.Clock(period=0.05)
mpc : qpgen.BaB(offset=0.018)

act : io.AbeWriter(channel=1)
ang : io.AbeReader(channel=1)
pos : io.AbeReader(channel=2)

dis : misc.PacedValueIterator( period=15.0, values=[0.0, 8.65])

mux : misc.Mux()
demux : misc.Demux()

/* Connections */
clock.tick > ang.tick, pos.tick, dis.tick
pos.value > mux.pos
ang.value > mux.ang
dis.value > mux.ref
mux.arr > demux.arr
demux.pos > mpc.position
demux.ang > mpc.angle
demux.ref > mpc.ref
mpc.u > act.value

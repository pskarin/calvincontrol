/* Actors */
clock : misc.Clock(period=0.05)
/*mpc : qpgen.BaB(offset=0.018)*/
mpc : qpgen.BaB(offset=0.05)

act : io.AbeWriter(channel=1)
ang : io.AbeReader(channel=1)
pos : io.AbeReader(channel=2)

dis : misc.PacedValueIterator( period=15.0, values=[0.0, 9.05])

/* Connections */
clock.tick > ang.tick, pos.tick, dis.tick
pos.value > mpc.position
ang.value > mpc.angle
mpc.u > act.value
dis.value > mpc.ref
